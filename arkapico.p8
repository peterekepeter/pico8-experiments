pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function reset()
 px=64
 pxs=0
 pw=16
 py=120
 balls={}
 speed=60
	for i=1,10,1 do
		balls[i]={		 
		 x=px,
		 y=py-8,
		 xf=px,yf=py-8,
		 xs=(rnd(4)-rnd(4))/4,
		 ys=-1,
		 
		 active=false,
		 hit=true
		}
	end
	for i=1,8,1 do
  puttile(2,i,0) 
  puttile(4,i,1) 
  puttile(6,i,2) 
  puttile(8,i,3) 
  puttile(10,i,4) 
  puttile(12,i,0) 
	end
	balls[1].active=true
end

function puttile(x,y,t)
  local tt=t*2+64;
  if (tt<80) tt+=flr(rnd(2))*16
  mset(x,y,tt)
  mset(x+1,y,tt+1)
end

function hittile(x,y,t)
 mset(x,y,0)
 mset(x+1,y,0)
 sfx(4+(flr(t/2))%2) 
 speed+=1
end

function chktile(x,y)
 local bx=flr(x/8)
 local by=flr(y/8)
	local bl=mget(bx,by)
	if (bl>=64 and bl<128) then
	 if(bl%2==1) bx-=1
	 hittile(bx,by,bl)
	 return true
	end
	return false
end 

function _update()
 -- player
 if (btn(0) and px>pw+6) pxs-=1
 if (btn(1) and px<121-pw) pxs+=1 
 px+=pxs
 pxs*=0.8
 if (px<pw+6) px=pw+6 pxs=0 sfx(3)
 if (px>121-pw) px=121-pw pxs=0 sfx(3)
 -- balls
 for ball in all(balls) do
  if (ball.active) then
   -- normalize speed
   local norm=0.05*speed/sqrt(ball.xs*ball.xs+ball.ys*ball.ys);
   ball.xs*=norm
   ball.ys*=norm
   if (chktile(ball.x+ball.xs, ball.y)) then
    ball.xs*=-1
   end
   if (chktile(ball.x,ball.y+ball.ys)) then
    ball.ys*=-1
   end
   ball.x+=ball.xs
	  ball.y+=ball.ys
	  -- boundary check
	  if (ball.x<8) ball.xs=abs(ball.xs) sfx(1)
	  if (ball.x>120) ball.xs=-abs(ball.xs) sfx(1)
	  if (ball.y<8) ball.ys=abs(ball.ys) sfx(2)
	  if (ball.y>136) ball.active=false
	  -- player check
	  if (ball.y>py and ball.y<py+6 and ball.x<px+pw and ball.x>px-pw) then
				if (not ball.hit) then
					ball.ys=-abs(ball.ys)	 
					ball.xs+=(ball.x-px)/pw*speed*0.05	
					if (abs(ball.xs*2)>abs(ball.ys)) ball.xs*=0.75
					sfx(0)
				end
				ball.hit=true
	  else 
	   ball.hit=false
	  end
	  -- block check
	  local bx=flr(ball.x/8)
	  local by=flr(ball.y/8)
	  local bl=mget(bx,by)
	  if (bl==12 or bl==28) then 
	  	mset(bx,by,0)
	  end
  end
 end
end

function drawball(bx,by)
	local sp=1
	 +flr(bx*2%2)*2
	 +flr(by*2%2);
 spr(sp,bx-4,by-4)
end

function _draw()
 -- bg
 rectfill(0,0,128,128,0)
 map()
 -- player
 --rect(pxf-pw+2,py,pxf+pw-2,py+4,7)
 sspr(48,0,8,8,px-pw+8,py,pw*2-15,8)
 spr(5,px-pw,py)
 spr(7,px+pw-7,py)
 -- balls
 for ball in all(balls) do
  if (ball.active) then
  ball.xf=(ball.x*4+ball.xf)*0.2
  ball.yf=(ball.y*4+ball.yf)*0.2
  drawball(ball.xf, ball.yf)
  drawball(ball.x, ball.y)
	  --local spx=(ball.x-flr(ball.x))
	  --spx=flr(spx*2)*2;
	  --local spy=(ball.y-flr(ball.y))
	  --spy=flr(spy*2);
	  --spr(1+spx+spy,ball.x-4,ball.y-4)
  end
 end
end

reset()
