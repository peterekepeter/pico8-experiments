pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function init_game()
	local i,j
	music(-1);
	scene_game=true;
	wsx=4;
	wsy=4;
	wex=4+7;
	wey=4+7;
 x=4;
 y=4;
 gx=x;
 gy=y;
 gsx=0;
 gsy=0;
 t=0;
 for i=0,16,1 do
 	for j=0,16,1 do
 		mset(i,j,2)
 	end
 end
 for i=wsx,wex,1 do
 	for j=wsy,wey,1 do
 		mset(i,j,0);
 	end
 end
end

function flp(x,y)
	if (x<wsx or x>wex) return 1
	if (y<wsy or y>wey) return 1
	if mget(x,y)==0 then
		mset(x,y,1);
		return 0;
	else 
	 mset(x,y,0);
	 return 1;
	end
end

function splat(x,y)
 local s=0;
 s+=flp(x,y);
	s+=flp(x+1,y);
	s+=flp(x-1,y);
	s+=flp(x,y+1);
	s+=flp(x,y-1);
	sfx(1+s);
end

function update_game()
	local s=0;
 if (btnp(0)) x-=1; gsx-=2; s=1;
 if (btnp(1)) x+=1; gsx+=2; s=1;
 if (btnp(2)) y-=1; gsy-=2; s=1;
 if (btnp(3)) y+=1; gsy+=2; s=1;
 if (x<wsx) x=wsx; s=2;
 if (x>wex) x=wex; s=2;
 if (y<wsy) y=wsy; s=2;
 if (y>wey) y=wey; s=2;
	if (s==1) sfx(0);
	if (s==2) sfx(7);
 if (btnp(4)) splat(x,y);
 t+=1;
 gsx=(gsx+x-gx)/1.75;
 gsy=(gsy+y-gy)/1.75;
 gx+=gsx/4;
 gy+=gsy/4;
end


function draw_game()
	cls(0);
	map()
	--if (flr(t/16)%2==1) then
		rect(gx*8+.5,gy*8+.5,gx*8+7+.5,gy*8+7+.5);
	--end
end

function init_menu()
	music(0);
	t=0;
	scene_game=false;
	selected=1;
	levelselect=false;
	gs1=1;
	gs2=1;
	gcx=-96;
	gcy=-63;
	gfx=0;
	gfy=0;
	pnote=-1;
	lvls = { 
		'tutorial',
		'level 1',
		'level 2',
		'level 3',
		'level 4',
		'level 5',
		'level 6',
		'level 7',
		'level 8',
		'random',
		'challenge',
		'impossible',
		'reset progress',
	}
end

function update_menu()
	if levelselect then
		local nextsel=selected;
	 if (btnp(2)) nextsel-=1;
	 if (btnp(3)) nextsel+=1;
	 if (btnp(4)) levelselect=false; sfx(3);
	 if lvls[nextsel]!=nil then
			if selected != nextsel then
		 	selected=nextsel;
		 	sfx(0);
			end
	 else
	 	sfx(7);
	 end
	else
		if (btnp(5)) levelselect=true, sfx(1)
	end
 gs1=gs1*0.6+selected*0.4;
 gs2=gs1*0.95+selected*0.05;
 if levelselect then
 	gcx=gcx*0.8;
 	gcy=gcy*0.8+0.2*(gs2*9-63+0.5);
 	
 else
 	gcx=gcx*0.8-96*0.2;
 	gcy=gcy*0.8-32*0.2;
 end
 
 local note=t/8;
 local pat=note/32;
 local drmpat=peek(0x3100+3+4*flr(pat%6))
 local patnote=flr(note)%32;
 local ndur=note-flr(note);
 local i,a,e,w
 a=peek(0x3200+2*patnote+1+68*drmpat);
	e=shr(band(a,0x70),4);
	w=band(a,1);
	
	gfx=gfx*0.8;
	gfy=gfy*0.8;
	
	if e==3 then
		if pnote!=patnote then
			if w>0 then
				gfx+=16;
			else
				gfy+=16;
			end
		end
	end
	pnote=patnote;
	
end



function draw_menu()
	cls(0);
	color(7);
	camera(gcx+0.5,gcy+0.5);
	local i;
	for i=0,16,1 do
		local off=sin(i/8+t/16)*gfx/2;
		pal(7,2+gfx/4);
		map(i,3,-100+i*8,12+off,1,4);
		pal(7,7);
		map(i,3,-100+i*8,
			10+gfy*0.2+off,1,4);
	end
		
	-- helper
	color(3);
	print("clear the board", 64,16-24+gcy/4);
	print("to finish a level",60,24-24+gcy/4);
	
	-- lvl select
	local x,y=48,0;
	  rect(
	  	x+6,gs1*9+.5-1,
	  	x+6+0*8+7+.5,gs1*9+7+.5-1);
	for i=1,#lvls do
	 local v=lvls[i];
	 if (v==nil) break;
	 color(6);
	 spr(3,x+6,9*i-1)
	 color(2);
 	print(v,16+x,9*i+1)
 	color(6);
	 if (i==selected) color(7);
 	print(v,16+x,9*i)
 	i+=1;
	end
end

function _update60()
	t+=1;
 if scene_game then
		update_game();
 else
		update_menu();
 end
end

function _draw()
 camera(0,0);
 if scene_game then
		draw_game();
 else
		draw_menu();
 end
end
 
t=0;
init_menu();
